{
  "name": "nestflix",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "strict-npm-engines && npm run clean",
    "prebuild": "strict-npm-engines && npm run clean",
    "clean": "rm -rf _site",
    "start:11ty": "eleventy --serve",
    "start:stylus": "stylus --watch ./src/css --out ./_site/css",
    "start": "concurrently -n stylus,11ty \"npm run start:stylus\" \"npm run start:11ty\"",
    "build:11ty": "eleventy",
    "build:stylus": "stylus --compress ./src/css --out ./_site/css",
    "build": "npm run build:stylus && npm run build:11ty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lynnandtonic/nestflix.git"
  },
  "bugs": {
    "url": "https://github.com/lynnandtonic/nestflix/issues"
  },
  "homepage": "https://github.com/lynnandtonic/nestflix#readme",
  "engines": {
    "node": ">=14",
    "npm": ">=6.14"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "any-shell-escape": "^0.1.1",
    "clean-css": "^5.1.2",
    "concurrently": "^6.0.2",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.2.0",
    "datauri": "^3.0.0",
    "eleventy-plugin-local-images": "^0.4.0",
    "ffmpeg-static": "^4.3.0",
    "fs-extra": "^9.1.0",
    "hasha": "^5.2.2",
    "html-minifier": "^4.0.0",
    "image-size": "^1.0.0",
    "jsdom": "^16.5.3",
    "luxon": "^1.26.0",
    "node-fetch": "^2.6.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "purgecss": "^4.0.3",
    "sharp": "^0.28.1",
    "shorthash": "0.0.2",
    "strict-npm-engines": "0.0.1",
    "stylus": "^0.54.8"
  }
}
